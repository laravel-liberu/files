{
    "name": "laravel-liberu/files",
    "description": "File manager dependency for Laravel Enso",
    "keywords": [
        "laravel-liberu",
        "files",
        "file-manager",
        "file-uploader"
    ],
    "homepage": "https://github.com/laravel-liberu/files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/image-transformer": "^2.0",
        "laravel-liberu/migrator": "^2.0",
        "laravel-liberu/track-who": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Files\\": "src/",
            "LaravelEnso\\Files\\Database\\Factories\\": "database/factories/",
            "LaravelEnso\\Files\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Files\\AppServiceProvider",
                "LaravelEnso\\Files\\AuthServiceProvider",
                "LaravelEnso\\Files\\EnumServiceProvider"
            ]
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
